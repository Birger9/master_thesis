@prefix : <http://solid-pod-5/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# --- Classes ---

:TransportVehicle a owl:Class ;
    rdfs:label "Transport Vehicle"@en ;
    rdfs:comment "A general class for transport vehicles"@en .

:TransportJob a owl:Class ;
    rdfs:label "Transport Job"@en ;
    rdfs:comment "A general class for transport jobs"@en .

# --- Properties ---

:vehicleID a owl:DatatypeProperty ; 
    rdfs:label "Vehicle ID"@en ;
    rdfs:comment "A unique identifier for a vehicle"@en ;
    rdfs:domain :TransportVehicle ; 
    rdfs:range xsd:string .

:vehicleType a owl:DatatypeProperty ; 
    rdfs:label "Vehicle Type"@en ;
    rdfs:comment "The type of the vehicle"@en ;
    rdfs:domain :TransportVehicle ; 
    rdfs:range rdfs:Literal .

:jobID a owl:DatatypeProperty ; 
    rdfs:label "Job ID"@en ;
    rdfs:comment "A unique identifier for a job"@en ;
    rdfs:domain :TransportJob ; 
    rdfs:range xsd:string .

:originCity a owl:DatatypeProperty ; 
    rdfs:label "Origin City"@en ;
    rdfs:comment "The orgin city"@en ;
    rdfs:domain :TransportJob ; 
    rdfs:range rdfs:Literal .

:destinationCity a owl:DatatypeProperty ; 
    rdfs:label "Destination City"@en ;
    rdfs:comment "The destination city"@en ;
    rdfs:domain :TransportJob ; 
    rdfs:range rdfs:Literal .

:scheduledDate a owl:DatatypeProperty ; 
    rdfs:label "Scheduled Date"@en ;
    rdfs:comment "The scheduled date for a job"@en ;
    rdfs:domain :TransportJob ; 
    rdfs:range xsd:date .

:assignedVehicle a owl:ObjectProperty ; 
    rdfs:label "Assigned Vehicle"@en ;
    rdfs:comment "The assigned vehicle to a job"@en ;
    rdfs:domain :TransportJob ; 
    rdfs:range :TransportVehicle .