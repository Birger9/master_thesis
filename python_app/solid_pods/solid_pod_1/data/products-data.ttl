@prefix lindnerOnt: <http://solid-pod-1/ontology#> .
@prefix con: <http://solid-pod-0/ontology#> .
@prefix lindnerData: <http://solid-pod-1/data/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

lindnerData:nortec-cs-tile
    a con:Component ;
    rdfs:label "NORTEC Raised Floor Tile"@en ;
    con:productID "NORTEC-CS-01" ;
    lindnerOnt:productFamily "NORTEC" ;
    lindnerOnt:productName "NORTEC Raised Floor Tile" ;
    con:material "Calcium Sulphate" ;
    con:pedestalGridSize "600x600 mm" ;
    con:isNonCombustible true ;
    con:systemWeightMin "32"^^xsd:decimal ;
    con:systemWeightMax "62"^^xsd:decimal ;
    con:hasLogisticsCondition lindnerData:TakeBackLogistics1 .   

lindnerData:nortec-steel-pedestal
    a con:Component ;
    rdfs:label "NORTEC Steel Pedestal H200"@en ;
    con:productID "NORTEC-PED-S" ;
    lindnerOnt:productFamily "NORTEC" ;
    lindnerOnt:productName "NORTEC Steel Pedestal H200" ;
    con:material "Galvanised Steel" ;
    con:isFullyRecyclable true .

lindnerData:lmd-ceiling-panel
    a con:Component ;
    rdfs:label "LMD Ceiling Panel"@en ;
    con:productID "LMD-CP-01" ; 
    lindnerOnt:productFamily "LMD" ;
    lindnerOnt:productName "LMD Ceiling Panel" ;
    con:material "Aluminum" .

lindnerData:nortec-floor-system
    a con:Product ;
    rdfs:label "NORTEC Raised Floor System"@en ;
    con:hasComponent lindnerData:nortec-cs-tile , 
                     lindnerData:nortec-steel-pedestal .

# --- Stakeholder Instances ---

lindnerData:Lindner_Group
    a con:Stakeholder ;
    rdfs:label "Lindner Group"@en ;
    con:paysSharedTruckCost false .          # Lindner benefits but does not pay

lindnerData:Concular
    a con:Stakeholder ;
    rdfs:label "Concular"@en ;
    con:paysSharedTruckCost true .           # Concular (transporter) bears the cost 

lindnerData:RangSells
    a con:Stakeholder ;
    rdfs:label "Rang-Sells"@en ;
    con:paysSharedTruckCost false .          # RangSells benefits but does not pay

# --- LogisticsCondition Instance ---

lindnerData:TakeBackLogistics1
    a con:LogisticsCondition ;
    rdfs:label "Take-Back Logistics for NORTEC"@en ;
    con:involvesStakeholder lindnerData:Lindner_Group ,
                            lindnerData:Concular ,
                            lindnerData:RangSells ;
    con:requiresTransportJob logisticsData:jobs-job-987 ;
    con:requiresTransportJob logisticsData:jobs-job-988 ;
    con:requiresSharedVehicle true ;
    con:transportConditionDescription
        "Tiles must be loaded onto a shared electric truck; return job must be booked at least two weeks in advance. " .
        
lindnerData:locations-frankfurt-when
    a con:StorageLocation ;
    rdfs:label "Lindner Frankfurt Warehouse"@en ;
    con:warehouseName "Frankfurt Warehouse" .

lindnerData:locations-munich-depot
    a con:StorageLocation ;
    rdfs:label "Lindner Munich Depot"@en ;
    con:warehouseName "Munich Depot"

lindnerData:items-batch-101
    a con:InventoryItem ;
    rdfs:label "Batch 101: Reusable NORTEC Tiles (Lindner's view)"@en ;
    con:itemIdentifier "BATCH-101-NORTEC-TILES" ;
    con:componentType lindnerData:nortec-cs-tile ;
    con:quantity 500 ;
    con:currentAssessment <http://solid-pod-2/data/assessments-assess-001> ;
    con:isAvailableForReuse true ;
    con:storedAt lindnerData:locations-frankfurt-wh ;
    con:meetsFireSafetyCriteria true .

lindnerData:items-batch-102
    a con:InventoryItem ;
    rdfs:label "Batch 102: Recyclable NORTEC Tiles (Lindner's view)"@en ;
    con:itemIdentifier "BATCH-102-NORTEC-TILES" ;
    con:componentType lindnerData:nortec-cs-tile ;
    con:quantity 250 ;
    con:currentAssessment <http://solid-pod-2/data/assessments-assess-002> ;
    con:isAvailableForReuse false ;
    con:storedAt lindnerData:locations-frankfurt-wh ;
    con:meetsFireSafetyCriteria true . 

lindnerData:items-batch-201
    a con:InventoryItem ;
    rdfs:label "Batch 201: Damaged LMD Panels (Lindner's view)"@en ;
    con:itemIdentifier "BATCH-201-LMD-PANELS" ;
    con:componentType lindnerData:lmd-ceiling-panel ;
    con:quantity 100 ;
    con:currentAssessment <http://solid-pod-2/data/assessments-assess-003> ;
    con:isAvailableForReuse false ;
    con:storedAt lindnerData:locations-munich-depot .

lindnerData:items-batch-301
    a con:InventoryItem ;
    rdfs:label "Batch 301: Damaged NORTEC Pedestals (Lindner's view)"@en ;
    con:itemIdentifier "BATCH-301-NORTEC-PEDESTALS" ;
    con:componentType lindnerData:nortec-steel-pedestal ;
    con:quantity 800 ;
    con:currentAssessment <http://solid-pod-2/data/assessments-assess-003> ;
    con:isAvailableForReuse false ;
    con:storedAt lindnerData:locations-munich-depot .









