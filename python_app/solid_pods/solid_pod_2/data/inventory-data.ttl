@prefix concularOnt: <http://solid-pod-2/ontology#> .
@prefix con: <http://solid-pod-0/ontology#> .
@prefix concularData: <http://solid-pod-2/data/> .
@prefix lindnerData: <http://solid-pod-1/data/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# Storage locations data
concularData:locations-frankfurt-wh
    a concularOnt:StorageLocation ;
    rdfs:label "Concular Frankfurt Warehouse"@en ;
    con:warehouseName "Frankfurt Warehouse" .

concularData:locations-munich-depot
    a concularOnt:StorageLocation ;
    rdfs:label "Concular Munich Depot"@en ;
    con:warehouseName "Munich Depot" .

# Assessments data
concularData:assessments-assess-001
    a concularOnt:ReusableAssessment ;
    rdfs:label "Assessment 001 (Result: Reusable)"@en .

concularData:assessments-assess-002
    a concularOnt:RecyclableAssessment ;
    rdfs:label "Assessment 002 (Result: Recyclable)"@en .

concularData:assessments-assess-003
    a concularOnt:DamagedAssessment ;
    rdfs:label "Assessment 003 (Result: Damaged)"@en .

# Inventory batches data
concularData:items-batch-101
    a concularOnt:InventoryItem ;
    rdfs:label "Batch 101: Reusable NORTEC Tiles"@en ;
    con:itemIdentifier "BATCH-101-NORTEC-TILES" ;
    con:componentType lindnerData:nortec-cs-tile ;
    con:quantity 500 ;
    con:currentAssessment concularData:assessments-assess-001 ;
    con:isAvailableForReuse true ;
    con:storedAt concularData:locations-frankfurt-wh .

concularData:items-batch-102
    a concularOnt:InventoryItem ;
    rdfs:label "Batch 102: Recyclable NORTEC Tiles"@en ;
    con:itemIdentifier "BATCH-102-NORTEC-TILES" ;
    con:componentType lindnerData:nortec-cs-tile ;
    con:quantity 250 ;
    con:currentAssessment concularData:assessments-assess-002 ;
    con:isAvailableForReuse false ;
    con:storedAt concularData:locations-frankfurt-wh .

concularData:items-batch-201
    a concularOnt:InventoryItem ;
    rdfs:label "Batch 201: Damaged LMD Panels"@en ;
    con:itemIdentifier "BATCH-201-LMD-PANELS" ;
    con:componentType lindnerData:lmd-ceiling-panel ;
    con:quantity 100 ;
    con:currentAssessment concularData:assessments-assess-003 ;
    con:isAvailableForReuse false ;
    con:storedAt concularData:locations-munich-depot .

concularData:items-batch-301
    a concularOnt:InventoryItem ;
    rdfs:label "Batch 301: Damaged NORTEC Pedestals"@en ;
    con:itemIdentifier "BATCH-301-NORTEC-PEDESTALS" ;
    con:componentType lindnerData:nortec-steel-pedestal ;
    con:quantity 800 ;
    con:currentAssessment concularData:assessments-assess-003 ;
    con:isAvailableForReuse false ;
    con:storedAt concularData:locations-munich-depot .
